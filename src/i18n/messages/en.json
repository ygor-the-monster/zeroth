{
	"$schema": "https://inlang.com/schema/inlang-message-format",
	"language_selector_button_label": "Switch language",
	"language_selector_dropdown_button_label": [
		{
			"declarations": ["input language"],
			"match": {
				"language=*": "Switch to {language}",
				"language=en": "Switch to English",
				"language=pt": "Switch to Brazilian Portuguese"
			},
			"selectors": ["language"]
		}
	]
}
