{
	"$schema": "https://inlang.com/schema/inlang-message-format",
	"language_selector_dropdown_button_label": [
		{
			"declarations": [
				"input language"
			],
			"selectors": [
				"language"
			],
			"match": {
				"language=*": "Mudar para {language}",
				"language=pt": "Mudar para Português Brasileiro",
				"language=en": "Mudar para Inglês"
			}
		}
	],
	"language_selector_button_label": "Mudar idioma"
}
