{
	"dependencies": {
		"@acab/reset.css": "^0.11.0",
		"@icons-pack/react-simple-icons": "^13.6.0",
		"@mdx-js/react": "^3.1.0",
		"@mdx-js/rollup": "^3.1.0",
		"dedent": "^1.6.0",
		"lucide-react": "^0.525.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.7.0",
		"remark-frontmatter": "^5.0.0",
		"remark-mdx-frontmatter": "^5.2.0",
		"usehooks-ts": "^3.1.1",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.0",
		"@cloudflare/vite-plugin": "^1.10.0",
		"@inlang/cli": "3.0.0",
		"@inlang/paraglide-js": "2.0.13",
		"@svgr/plugin-svgo": "^8.1.0",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/mdx": "^2.0.13",
		"@types/node": "^22.16.5",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@typescript/native-preview": "7.0.0-dev.20250722.1",
		"@vitejs/plugin-react-oxc": "^0.2.3",
		"onchange": "^7.1.0",
		"type-fest": "^4.41.0",
		"vite": "npm:rolldown-vite@^7.0.10",
		"vite-plugin-favicons": "^0.1.7",
		"vite-plugin-svgr": "^4.3.0",
		"wrangler": "^4.25.1"
	},
	"name": "zeroth-sprint",
	"pnpm": {
		"overrides": {
			"typescript": "npm:@typescript/native-preview@latest",
			"vite": "npm:rolldown-vite@latest"
		}
	},
	"private": true,
	"scripts": {
		"build": "pnpm build:prod",
		"build:dev": "vite build --mode development",
		"build:prod": "vite build --mode production",
		"deploy": "pnpm build && pnpm deploy:pages",
		"deploy:pages": "wrangler deploy",
		"dev": "pnpm run \"/^dev:.*/\"",
		"dev:app": "vite --mode development",
		"dev:formatter": "onchange -i \"src/*.tsx\" \"src/**/*.(ts|tsx|css|json)\" -- biome check --write \"{{file}}\"",
		"dev:typescript": "tsgo -p tsconfig.app.json --watch --noEmit",
		"lint": "pnpm lint:types && pnpm lint:check",
		"lint:check": "biome check --write .",
		"lint:format": "biome format --write .",
		"lint:types": "tsgo -p tsconfig.app.json --noEmit && tsgo -p tsconfig.node.json --noEmit",
		"preview": "pnpm build && pnpm preview:prod",
		"preview:dev": "vite preview --mode development",
		"preview:prod": "vite preview --mode production",
		"start": "pnpm start:dev",
		"start:dev": "vite --mode development",
		"start:prod": "vite --mode production",
		"translate": "inlang machine translate --project i18n.inlang"
	},
	"type": "module",
	"version": "0.0.0"
}
